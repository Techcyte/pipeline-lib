digraph search {
overlap = false;
#rankdir="LR";

# compute nodes
    node [
        shape=rect
        style=filled
        fillcolor="white"
        width=1.5
        height=0.75
        fixedsize=true
        pad="0.212,1"
    ]
    t1 [label="Task 1", fillcolor="white"]
    m1 [label="Queue 1", fillcolor="gray"]
    t2 [label="Task 2", fillcolor="white"]
    m2 [label="Queue 2", fillcolor="gray"]
    t3 [label="Task 3", fillcolor="gray"]

    t1 -> m1 [color="white"]
    m1 -> t2 [color="white"]
    t2 -> m2 [color="white"]
    m2 -> t3 [color="white"]
    t3 -> m2 [color="black"]
    t2 -> m1 [color="gray"]
    t1 -> m1 [color="red"]
    t2 -> m2 [color="red"]
}